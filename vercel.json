{
  "version": 2,
  "regions": ["hkg1", "sfo1" ,"pek1", "syd1","sgp1","fra1","lon1","nyc1","ams1","sin1","tor1","bom1","mel1","sao1"],
  "builds": [
    {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist"  
        }
    } 
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
        "src": "/images/(.*)",
        "headers": {
          "Cache-Control": "public, max-age=31536000, immutable"
        },
        "dest": "/images/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1",
      "headers": {
        "X-CDN-Provider": "TencentCloud",
        "X-China-Cache": "force"
      }
    }
  ]
}